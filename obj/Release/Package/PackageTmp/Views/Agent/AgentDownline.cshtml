
@{
    ViewBag.Title = "AgentDownline";
    Layout = "~/Views/Shared/AdminLayoutPage.cshtml";
}

<div class="card shadow mb-4">
	<div class="card-header py-3">
		<h5 class="m-0 font-weight-bold text-primary">Plot Details</h5>
	</div>
	<div class="card-body">
		<div class="row mt-3 mb-3">
			<div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
				<label>From Date</label>
				<input type="date" class="form-control" />
			</div>
			<div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
				<label>To Date</label>
				<input type="date" class="form-control" />
			</div>
			<div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
				<input type="submit" class="btn btn-info mt-4" value="Search" />
				<input type="reset" class="btn btn-info mt-4" value="Reset" />
			</div>
		</div>
		<div class="table-responsive">
			<div id="dataTable_wrapper" class="dataTables_wrapper dt-bootstrap4">
				<div class="row">
					<div class="col-sm-12">
						<table class="table table-bordered dataTable" width="100%" cellspacing="0" role="grid" aria-describedby="dataTable_info" style="width: 100%;">
							<thead>
								<tr role="row">
									<th>Sr. No.</th>
									<th>Site Name</th>
									<th>Location</th>
									<th>No of Plots</th>
									<th>Block</th>
									<th>Plot Type</th>
									<th>Plot No</th>
									<th>Status</th>
									<th>Rate</th>
									<th>Action</th>
								</tr>
							</thead>
							<tbody>
								<tr>
									<td>1</td>
									<td>Site 1</td>
									<td>LUCKNOW</td>
									<td>0</td>
									<td>
										<span id="ContentPlaceHolder1_GridView2_lblblock_0">SECTOR-A</span>
									</td>
									<td>
										<span id="ContentPlaceHolder1_GridView2_lblplottype_0">CORNER+MAIN ROAD+PARK</span>
									</td>
									<td>
										<span id="ContentPlaceHolder1_GridView2_lblplotnum_0">SECTOR-A-</span>
									</td>
									<td>Available</td>
									<td>450.00</td>
									<td align="center">
										<a href="javascript:void(0)" class="btn btn-sm text-primary" onclick="_update('')"><i class="fa fa-edit"></i></a>
										<a href="javascript:void(0)" class="btn btn-sm text-danger" onclick="_delete('')"><i class="fa fa-trash"></i></a>
									</td>
								</tr>

							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

