
@{
    ViewBag.Title = "AgentDirect";
    Layout = "~/Views/Shared/AdminLayoutPage.cshtml";
}

<div class="card shadow mb-4">
	<div class="card-header py-3">
		<h5 class="m-0 font-weight-bold text-primary">Agent Direct Details</h5>
	</div>
	<div class="card-body">
		<div class="row mt-3 mb-3">
			<div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
				<label>From Date</label>
				<input type="date" class="form-control" />
			</div>
			<div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
				<label>To Date</label>
				<input type="date" class="form-control" />
			</div>
			<div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
				<input type="submit" class="btn btn-success mt-4" value="Search" />
				<input type="reset" class="btn btn-danger mt-4" value="Reset" />
			</div>
		</div>
		<div class="table-responsive">
			<div id="dataTable_wrapper" class="dataTables_wrapper dt-bootstrap4">
				<div class="row">
					<div class="col-sm-12">
						<table class="table table-bordered dataTable" width="100%" cellspacing="0" role="grid" aria-describedby="dataTable_info" style="width: 100%;">
							<thead>
								<tr role="row">
									<th>Sr. No.</th>
									<th>Agent Id</th>
									<th>Agent Name</th>
									<th>Sponsor Id</th>
									<th>Sponsor Name</th>
									<th>Mobile No.</th>
									<th>Joining Date</th>
								</tr>
							</thead>
							<tbody>
								<tr>
									<td>
										1
									</td>
									<td>
										<a class="btn btn-primary" href="javascript:void(0)" style="color:White;">0999999</a>
									</td>
									<td>S DREAM WORLD</td>
									<td>888888</td>
									<td>&nbsp;</td>
									<td>000000000</td>
									<td>27 August, 2019</td>
								</tr>

							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

