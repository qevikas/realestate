
@{
    ViewBag.Title = "Enquiriy Details";
    Layout = "~/Views/Shared/AdminLayoutPage.cshtml";
}


<div class="card shadow mb-4">
	<div class="card-header py-3">
		<h5 class="m-0 font-weight-bold text-primary">Enquiry Details</h5>
	</div>
	<div class="card-body">
		<div class="table-responsive">
			<div id="dataTable_wrapper" class="dataTables_wrapper dt-bootstrap4">
				<div class="row">
					<div class="col-sm-12">
						<table class="table table-bordered dataTable" width="100%" cellspacing="0" role="grid" aria-describedby="dataTable_info" style="width: 100%;">
							<thead>
								<tr role="row">
									<th>Sr. No.</th>
									<th>Name</th>
									<th>Mobile No</th>
									<th>Email</th>
									<th>Subject</th>
									<th>Message</th>
								</tr>
							</thead>
							<tbody>
								<tr role="row">
									<td>1</td>
									<td>Demo</td>
									<td>9876543210</td>
									<td>example@gmail.con</td>
									<td>Want a service in Lucknow</td>
									<td>xyz</td>
								</tr>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

